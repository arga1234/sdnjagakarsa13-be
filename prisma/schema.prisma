generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Registrasi {
  id              String   @id @default(cuid())
  nama            String
  jenisKelamin    String
  tempatLahir     String
  tanggalLahir    String
  nik             String
  kk              String
  agreement       Boolean
  createdAt       DateTime @default(now())
}
